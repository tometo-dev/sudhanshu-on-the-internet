    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2",
