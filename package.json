    "clsx": "2.1.1",
    "isbot": "4.4.0",
    "miniflare": "^3.20240419.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240502.0",
    "@remix-run/dev": "2.9.1",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
